
mixin example
  .shadow.rounded-lg.bg-white&attributes(attributes)
    block

mixin exampleDisplay
  .p-6.rounded-t-lg.flex.gap-4&attributes(attributes)
    block

mixin exampleSourceCode
  .p-6.bg-sky-lighter.rounded-b-lg.overflow-auto&attributes(attributes)
    pre
      code.text-md(style="font-family: 'Source Code Pro', monospace;")
        block

mixin section
  +hr
  +container
    block

mixin hr
  hr.border-t.border-t-sky.border-b.border-b-white